  [
    "dnsmasq-2.63.orig/debian/init", 
    "dnsmasq-2.63/debian/init", 
    [
      "--- dnsmasq-2.63.orig/debian/init", 
      "+++ dnsmasq-2.63/debian/init", 
      "@@ -0,0 +1,290 @@", 
      "+#!/bin/sh", 
      "+### BEGIN INIT INFO", 
      "+# Provides:       dnsmasq", 
      "+# Required-Start: $network $remote_fs $syslog", 
      "+# Required-Stop:  $network $remote_fs $syslog", 
      "+# Default-Start:  2 3 4 5", 
      "+# Default-Stop:   0 1 6", 
      "+# Description:    DHCP and DNS server", 
      "+### END INIT INFO", 
      "+", 
      "+set +e   # Don't exit on error status", 
      "+", 
      "+PATH=/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin", 
      "+DAEMON=/usr/sbin/dnsmasq", 
      "+NAME=dnsmasq", 
      "+DESC=\"DNS forwarder and DHCP server\"", 
      "+", 
      "+# Most configuration options in /etc/default/dnsmasq are deprecated", 
      "+# but still honoured.", 
      "+ENABLED=1", 
      "+if [ -r /etc/default/$NAME ]; then", 
      "+\t. /etc/default/$NAME", 
      "+fi", 
      "+", 
      "+# Get the system locale, so that messages are in the correct language, and the", 
      "+# charset for IDN is correct", 
      "+if [ -r /etc/default/locale ]; then", 
      "+        . /etc/default/locale", 
      "+        export LANG", 
      "+fi", 
      "+", 
      "+test -x $DAEMON || exit 0", 
      "+", 
      "+# Provide skeleton LSB log functions for backports which don't have LSB functions.", 
      "+if [ -f /lib/lsb/init-functions ]; then", 
      "+         . /lib/lsb/init-functions", 
      "+else", 
      "+         log_warning_msg () {", 
      "+            echo \"${@}.\"", 
      "+         }", 
      "+", 
      "+         log_success_msg () {", 
      "+            echo \"${@}.\"", 
      "+         }", 
      "+", 
      "+         log_daemon_msg () {", 
      "+            echo -n \"${1}: $2\"", 
      "+         }", 
      "+", 
      "+\t log_end_msg () {", 
      "+            if [ $1 -eq 0 ]; then", 
      "+              echo \".\"", 
      "+            elif [ $1 -eq 255 ]; then", 
      "+              /bin/echo -e \" (warning).\"", 
      "+            else", 
      "+              /bin/echo -e \" failed!\"", 
      "+            fi", 
      "+         }", 
      "+fi", 
      "+", 
      "+# RESOLV_CONF:", 
      "+# If the resolvconf package is installed then use the resolv conf file", 
      "+# that it provides as the default.  Otherwise use /etc/resolv.conf as", 
      "+# the default.", 
      "+#", 
      "+# If IGNORE_RESOLVCONF is set in /etc/default/dnsmasq or an explicit", 
      "+# filename is set there then this inhibits the use of the resolvconf-provided", 
      "+# information.", 
      "+#", 
      "+# Note that if the resolvconf package is installed it is not possible to", 
      "+# override it just by configuration in /etc/dnsmasq.conf, it is necessary", 
      "+# to set IGNORE_RESOLVCONF=yes in /etc/default/dnsmasq.", 
      "+", 
      "+if [ ! \"$RESOLV_CONF\" ] &&", 
      "+   [ \"$IGNORE_RESOLVCONF\" != \"yes\" ] &&", 
      "+   [ -x /sbin/resolvconf ]", 
      "+then", 
      "+\tRESOLV_CONF=/var/run/dnsmasq/resolv.conf", 
      "+fi", 
      "+", 
      "+for INTERFACE in $DNSMASQ_INTERFACE; do", 
      "+\tDNSMASQ_INTERFACES=\"$DNSMASQ_INTERFACES -i $INTERFACE\"", 
      "+done", 
      "+", 
      "+for INTERFACE in $DNSMASQ_EXCEPT; do", 
      "+\tDNSMASQ_INTERFACES=\"$DNSMASQ_INTERFACES -I $INTERFACE\"", 
      "+done", 
      "+", 
      "+if [ ! \"$DNSMASQ_USER\" ]; then", 
      "+   DNSMASQ_USER=\"dnsmasq\"", 
      "+fi", 
      "+", 
      "+start()", 
      "+{", 
      "+        # Return", 
      "+\t#   0 if daemon has been started", 
      "+\t#   1 if daemon was already running", 
      "+\t#   2 if daemon could not be started", 
      "+", 
      "+        # /var/run may be volatile, so we need to ensure that", 
      "+        # /var/run/dnsmasq exists here as well as in postinst", 
      "+        if [ ! -d /var/run/dnsmasq ]; then", 
      "+           mkdir /var/run/dnsmasq || return 2", 
      "+           chown dnsmasq:nogroup /var/run/dnsmasq || return 2", 
      "+        fi", 
      "+", 
      "+\tstart-stop-daemon --start --quiet --pidfile /var/run/dnsmasq/$NAME.pid --exec $DAEMON --test > /dev/null || return 1", 
      "+\tstart-stop-daemon --start --quiet --pidfile /var/run/dnsmasq/$NAME.pid --exec $DAEMON -- \\", 
      "+\t\t-x /var/run/dnsmasq/$NAME.pid \\", 
      "+\t        ${MAILHOSTNAME:+ -m $MAILHOSTNAME} \\", 
      "+\t\t${MAILTARGET:+ -t $MAILTARGET} \\", 
      "+\t\t${DNSMASQ_USER:+ -u $DNSMASQ_USER} \\", 
      "+\t\t${DNSMASQ_INTERFACES:+ $DNSMASQ_INTERFACES} \\", 
      "+\t\t${DHCP_LEASE:+ -l $DHCP_LEASE} \\", 
      "+\t\t${DOMAIN_SUFFIX:+ -s $DOMAIN_SUFFIX} \\", 
      "+\t\t${RESOLV_CONF:+ -r $RESOLV_CONF} \\", 
      "+\t\t${CACHESIZE:+ -c $CACHESIZE} \\", 
      "+\t        ${CONFIG_DIR:+ -7 $CONFIG_DIR} \\", 
      "+\t\t${DNSMASQ_OPTS:+ $DNSMASQ_OPTS} \\", 
      "+\t\t|| return 2", 
      "+}", 
      "+", 
      "+start_resolvconf()", 
      "+{", 
      "+# If interface \"lo\" is explicitly disabled in /etc/default/dnsmasq", 
      "+# Then dnsmasq won't be providing local DNS, so don't add it to", 
      "+# the resolvconf server set.", 
      "+\tfor interface in $DNSMASQ_EXCEPT", 
      "+\tdo", 
      "+\t\t[ $interface = lo ] && return", 
      "+\tdone", 
      "+", 
      "+        if [ -x /sbin/resolvconf ] ; then", 
      "+\t\techo \"nameserver 127.0.0.1\" | /sbin/resolvconf -a lo.$NAME", 
      "+\tfi", 
      "+\treturn 0", 
      "+}", 
      "+", 
      "+stop()", 
      "+{", 
      "+\t# Return", 
      "+\t#   0 if daemon has been stopped", 
      "+\t#   1 if daemon was already stopped", 
      "+\t#   2 if daemon could not be stopped", 
      "+\t#   other if a failure occurred", 
      "+\tstart-stop-daemon --stop --quiet --retry=TERM/30/KILL/5 --pidfile /var/run/dnsmasq/$NAME.pid --name $NAME", 
      "+\tRETVAL=\"$?\"", 
      "+\t[ \"$RETVAL\" = 2 ] && return 2", 
      "+\treturn \"$RETVAL\"", 
      "+}", 
      "+", 
      "+stop_resolvconf()", 
      "+{", 
      "+\tif [ -x /sbin/resolvconf ] ; then", 
      "+\t\t/sbin/resolvconf -d lo.$NAME", 
      "+\tfi", 
      "+\treturn 0", 
      "+}", 
      "+", 
      "+status()", 
      "+{", 
      "+\t# Return", 
      "+\t#   0 if daemon is running", 
      "+\t#   1 if daemon is dead and pid file exists", 
      "+\t#   3 if daemon is not running", 
      "+\t#   4 if daemon status is unknown", 
      "+\tstart-stop-daemon --start --quiet --pidfile /var/run/dnsmasq/$NAME.pid --exec $DAEMON --test > /dev/null", 
      "+\tcase \"$?\" in", 
      "+\t\t0) [ -e \"/var/run/dnsmasq/$NAME.pid\" ] && return 1 ; return 3 ;;", 
      "+\t\t1) return 0 ;;", 
      "+\t\t*) return 4 ;;", 
      "+\tesac", 
      "+}", 
      "+", 
      "+case \"$1\" in", 
      "+  start)", 
      "+\ttest \"$ENABLED\" != \"0\" || exit 0", 
      "+\tlog_daemon_msg \"Starting $DESC\" \"$NAME\"", 
      "+\tstart", 
      "+\tcase \"$?\" in", 
      "+\t\t0)", 
      "+\t\t\tlog_end_msg 0", 
      "+\t\t\tstart_resolvconf", 
      "+\t\t\texit 0", 
      "+\t\t\t;;", 
      "+\t\t1)", 
      "+\t\t\tlog_success_msg \"(already running)\"", 
      "+\t\t\texit 0", 
      "+\t\t\t;;", 
      "+\t\t*)", 
      "+\t\t\tlog_end_msg 1", 
      "+\t\t\texit 1", 
      "+\t\t\t;;", 
      "+\tesac", 
      "+\t;;", 
      "+  stop)", 
      "+\tstop_resolvconf", 
      "+\tif [ \"$ENABLED\" != \"0\" ]; then", 
      "+             log_daemon_msg \"Stopping $DESC\" \"$NAME\"", 
      "+\tfi", 
      "+\tstop", 
      "+        RETVAL=\"$?\"", 
      "+\tif [ \"$ENABLED\" = \"0\" ]; then", 
      "+\t    case \"$RETVAL\" in", 
      "+\t       0) log_daemon_msg \"Stopping $DESC\" \"$NAME\"; log_end_msg 0 ;;", 
      "+            esac", 
      "+\t    exit 0", 
      "+\tfi", 
      "+\tcase \"$RETVAL\" in", 
      "+\t\t0) log_end_msg 0 ; exit 0 ;;", 
      "+\t\t1) log_warning_msg \"(not running)\" ; exit 0 ;;", 
      "+\t\t*) log_end_msg 1; exit 1 ;;", 
      "+\tesac", 
      "+\t;;", 
      "+  restart|force-reload)", 
      "+\ttest \"$ENABLED\" != \"0\" || exit 1", 
      "+\t$DAEMON --test ${CONFIG_DIR:+ -7 $CONFIG_DIR} ${DNSMASQ_OPTS:+ $DNSMASQ_OPTS} >/dev/null 2>&1", 
      "+\tif [ $? -ne 0 ]; then", 
      "+\t    NAME=\"configuration syntax check\"", 
      "+\t    RETVAL=\"2\"", 
      "+\telse", 
      "+\t    stop_resolvconf", 
      "+\t    stop", 
      "+\t    RETVAL=\"$?\"", 
      "+        fi", 
      "+\tlog_daemon_msg \"Restarting $DESC\" \"$NAME\"", 
      "+\tcase \"$RETVAL\" in", 
      "+\t\t0|1)", 
      "+\t\t        sleep 2", 
      "+\t\t\tstart", 
      "+\t\t\tcase \"$?\" in", 
      "+\t\t\t\t0)", 
      "+\t\t\t\t\tlog_end_msg 0", 
      "+\t\t\t\t\tstart_resolvconf", 
      "+\t\t\t\t\texit 0", 
      "+\t\t\t\t\t;;", 
      "+\t\t\t        *)", 
      "+\t\t\t\t\tlog_end_msg 1", 
      "+\t\t\t\t\texit 1", 
      "+\t\t\t\t\t;;", 
      "+\t\t\tesac", 
      "+\t\t\t;;", 
      "+\t\t*)", 
      "+\t\t\tlog_end_msg 1", 
      "+\t\t\texit 1", 
      "+\t\t\t;;", 
      "+\tesac", 
      "+\t;;", 
      "+  status)", 
      "+\tlog_daemon_msg \"Checking $DESC\" \"$NAME\"", 
      "+\tstatus", 
      "+\tcase \"$?\" in", 
      "+\t\t0) log_success_msg \"(running)\" ; exit 0 ;;", 
      "+\t\t1) log_success_msg \"(dead, pid file exists)\" ; exit 1 ;;", 
      "+\t\t3) log_success_msg \"(not running)\" ; exit 3 ;;", 
      "+\t\t*) log_success_msg \"(unknown)\" ; exit 4 ;;", 
      "+\tesac", 
      "+\t;;", 
      "+  dump-stats)", 
      "+        kill -s USR1 `cat /var/run/dnsmasq/$NAME.pid`", 
      "+\t;;", 
      "+  systemd-start-resolvconf)", 
      "+\tstart_resolvconf", 
      "+\t;;", 
      "+  systemd-stop-resolvconf)", 
      "+\tstop_resolvconf", 
      "+\t;;", 
      "+  systemd-exec)", 
      "+#  --pid-file without argument disables writing a PIDfile, we don't need one with sytemd.", 
      "+# Enable DBus by default because we use DBus activation with systemd.", 
      "+\texec $DAEMON --keep-in-foreground --pid-file --enable-dbus \\", 
      "+\t    ${MAILHOSTNAME:+ -m $MAILHOSTNAME} \\", 
      "+\t    ${MAILTARGET:+ -t $MAILTARGET} \\", 
      "+\t    ${DNSMASQ_USER:+ -u $DNSMASQ_USER} \\", 
      "+\t    ${DNSMASQ_INTERFACES:+ $DNSMASQ_INTERFACES} \\", 
      "+\t    ${DHCP_LEASE:+ -l $DHCP_LEASE} \\", 
      "+\t    ${DOMAIN_SUFFIX:+ -s $DOMAIN_SUFFIX} \\", 
      "+\t    ${RESOLV_CONF:+ -r $RESOLV_CONF} \\", 
      "+\t    ${CACHESIZE:+ -c $CACHESIZE} \\", 
      "+\t    ${CONFIG_DIR:+ -7 $CONFIG_DIR} \\", 
      "+\t    ${DNSMASQ_OPTS:+ $DNSMASQ_OPTS}", 
      "+\t;;", 
      "+  *)", 
      "+\techo \"Usage: /etc/init.d/$NAME {start|stop|restart|force-reload|dump-stats|status}\" >&2", 
      "+\texit 3", 
      "+\t;;", 
      "+esac", 
      "+", 
      "+exit 0", 
      "+"
    ]
  ], 
