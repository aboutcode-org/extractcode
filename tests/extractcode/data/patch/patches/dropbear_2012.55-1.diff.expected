  [
    "dropbear-2012.55.orig/debian/initramfs/dropbear-hook", 
    "dropbear-2012.55/debian/initramfs/dropbear-hook", 
    [
      "--- dropbear-2012.55.orig/debian/initramfs/dropbear-hook", 
      "+++ dropbear-2012.55/debian/initramfs/dropbear-hook", 
      "@@ -0,0 +1,52 @@", 
      "+#!/bin/sh", 
      "+", 
      "+PREREQ=\"\"", 
      "+", 
      "+prereqs() {", 
      "+\techo \"$PREREQ\"", 
      "+}", 
      "+", 
      "+case \"$1\" in", 
      "+\tprereqs)", 
      "+\t\tprereqs", 
      "+\t\texit 0", 
      "+\t;;", 
      "+esac", 
      "+", 
      "+. \"${CONFDIR}/initramfs.conf\"", 
      "+. /usr/share/initramfs-tools/hook-functions", 
      "+", 
      "+# Install dropbear if explicitly enabled, or in case of a cryptroot setup if not explicitly disabled", 
      "+if [ \"${DROPBEAR}\" = \"y\" ] || ( [ \"${DROPBEAR}\" != \"n\" ] && [ -r \"/etc/crypttab\" ] ); then", 
      "+\tif [ ! -x \"/usr/sbin/dropbear\" ]; then", 
      "+\t\tif [ \"${DROPBEAR}\" = \"y\" ]; then", 
      "+\t\t\techo \"dropbear: FAILURE: Dropbear not found!\"", 
      "+\t\telse", 
      "+\t\t\techo \"dropbear: WARNING: Dropbear not found, remote unlocking of cryptroot via ssh won't work!\"", 
      "+\t\tfi", 
      "+\telse", 
      "+\t\trm -f \"${DESTDIR}/sbin/dropbear\"", 
      "+\t\tcopy_exec \"/usr/sbin/dropbear\" \"/sbin/\"", 
      "+\t\tcp /lib/libnss_* \"${DESTDIR}/lib/\"", 
      "+\t\techo \"root:x:0:0:root:/root:/bin/sh\" > \"${DESTDIR}/etc/passwd\"", 
      "+\t\tfor keytype in \"dss\" \"rsa\"; do", 
      "+\t\t\tif [ ! -f \"/etc/initramfs-tools/etc/dropbear/dropbear_${keytype}_host_key\" ]; then", 
      "+\t\t\t\tmkdir -p \"/etc/initramfs-tools/etc/dropbear\"", 
      "+\t\t\t\tdropbearkey -t \"${keytype}\" -f \"/etc/initramfs-tools/etc/dropbear/dropbear_${keytype}_host_key\"", 
      "+\t\t\tfi", 
      "+\t\tdone", 
      "+\t\tcp -R /etc/initramfs-tools/etc/dropbear \"${DESTDIR}/etc/\"", 
      "+\t\tif [ ! -f \"/etc/initramfs-tools/root/.ssh/authorized_keys\" ]; then", 
      "+\t\t\tmkdir -p \"/etc/initramfs-tools/root/.ssh\"", 
      "+\t\t\tif [ ! -f \"/etc/initramfs-tools/root/.ssh/id_rsa.pub\" ]; then", 
      "+\t\t\t\tdropbearkey -t rsa -f /etc/initramfs-tools/root/.ssh/id_rsa.dropbear", 
      "+\t\t\t\t/usr/lib/dropbear/dropbearconvert dropbear openssh /etc/initramfs-tools/root/.ssh/id_rsa.dropbear /etc/initramfs-tools/root/.ssh/id_rsa", 
      "+\t\t\t\tdropbearkey -y -f /etc/initramfs-tools/root/.ssh/id_rsa.dropbear | grep \"^ssh-rsa \" > /etc/initramfs-tools/root/.ssh/id_rsa.pub", 
      "+\t\t\tfi", 
      "+\t\t\tcat /etc/initramfs-tools/root/.ssh/id_rsa.pub >> /etc/initramfs-tools/root/.ssh/authorized_keys", 
      "+\t\tfi", 
      "+\t\tmkdir -p \"${DESTDIR}/root/.ssh\"", 
      "+\t\tcp /etc/initramfs-tools/root/.ssh/authorized_keys \"${DESTDIR}/root/.ssh/\"", 
      "+\tfi", 
      "+fi", 
      "+"
    ]
  ], 
